		<attribute>
			<name>topPos</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>outbarWidth</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>outbarHeight</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>							
	</tag>
	
	<tag>
		<name>outlookBar-body</name>
