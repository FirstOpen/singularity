		<tagclass>com.pragmaticobjects.taglibs.tree.TreeHeadTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>A tree head tag</info>							
	</tag>
	
	<tag>
		<name>tree-body</name>
		<tagclass>com.pragmaticobjects.taglibs.tree.TreeBodyTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>A tree body tag</info>
		<attribute>
			<name>treeConfigXmlPath</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
